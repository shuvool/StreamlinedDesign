<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>


<Pane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="sample.ServiceTypeStatusController">
    <HBox>
        <VBox>
            <Button fx:id="serviceStatus" text="Status" />
            <Button fx:id="serviceType" text="Type" />
        </VBox>
        <VBox>
            <TableView fx:id="tableView" GridPane.columnIndex = "0" GridPane.rowIndex = "1">
                    <columns>
                        <TableColumn text="ID"></TableColumn>
                        <TableColumn text="First Name"></TableColumn>
                        <TableColumn text="Last Name"></TableColumn>
                        <TableColumn text="Street Number"></TableColumn>
                        <TableColumn text="Street Name"></TableColumn>
                        <TableColumn text="City"></TableColumn>
                        <TableColumn text="Service Status"></TableColumn>
                    </columns>

        </VBox>
    </HBox>
</Pane>