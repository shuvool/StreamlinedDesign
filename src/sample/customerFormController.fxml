        <?xml version="1.0" encoding="UTF-8"?>

        <?import javafx.geometry.Insets?>
        <?import javafx.scene.control.ListView?>
        <?import javafx.scene.control.TextField?>
        <?import javafx.scene.layout.Pane?>


        <?import javafx.scene.layout.HBox?>

<Pane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="sample.CustomerFormController">
    <ListView layoutX="142.0" layoutY="100.0" prefHeight="200.0" prefWidth="316.0"/>
    <VBox>
        <HBox prefHeight="400">
            <TextField fx:id="firstName" layoutX="226.0" layoutY="100.0" promptText="First Name">
                <padding>
                    <Insets bottom="5.0" top="5.0"/>
                </padding>
            </TextField>

            <TextField fx:id="lastName" layoutX="226.0" layoutY="125.0" promptText="Last Name">
                <padding>
                    <Insets bottom="5.0" top="5.0"/>
                </padding>
            </TextField>

            <TextField fx:id="streetNumber" layoutX="226.0" layoutY="150.0" promptText="Street Number">
                <padding>
                    <Insets bottom="5.0" top="5.0"/>
                </padding>
            </TextField>

            <TextField fx:id="streetName" layoutX="226.0" layoutY="175.0" promptText="Street Name">
                <padding>
                    <Insets bottom="5.0" top="5.0"/>
                </padding>
            </TextField>

            <TextField fx:id="city" layoutX="226.0" layoutY="200.0" promptText="City">
                <padding>
                    <Insets bottom="5.0" top="5.0"/>
                </padding>
            </TextField>

            <TextField fx:id="state" layoutX="226.0" layoutY="225.0" promptText="State">
                <padding>
                    <Insets bottom="5.0" top="5.0"/>
                </padding>
            </TextField>

            <TextField fx:id="zipCode" layoutX="226.0" layoutY="250.0" promptText="Zip Code">
                <padding>
                    <Insets bottom="5.0" top="5.0"/>
                </padding>
            </TextField>
        </HBox>
        <Button fx:id="submit" text="Submit" />
    </VBox>
</Pane>
